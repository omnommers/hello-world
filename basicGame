def game_options():
	print("Head North")
	print("Head South")
	print('Exit')
	user_choice = input('what option')
	if user_choice == "exit":
		exit
	elif user_choice == "north":
		print("You head North.")
	elif user_choice == "south":
		print("You travel South.")
	else:
		print("Fool, that's not an option. Try again.")
		game_options()
